{
	"database": {
		"rules": "database.rules.json"
	},
	"hosting": [
		{
			"public": "build",
			"site": "intuition-writ-of-persona",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		},
		{
			"public": "dist",
			"site": "intuition-writ-of-persona-staging",
			"ignore": [
				"firebase.json",
				"**/.*",
				"**/node_modules/**"
			],
			"rewrites": [
				{
					"source": "**",
					"destination": "/index.html"
				}
			]
		}
	]
}